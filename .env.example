# Copy this file to .env and fill in your API keys

# OpenAI API Key (required for OpenAI models and vector stores)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_VECTOR_STORE_ID=your_vector_store_id_here

# CustomGPT API credentials (required for RAG evaluation)
CUSTOMGPT_API_KEY=your_customgpt_api_key_here
CUSTOMGPT_PROJECT=your_customgpt_project_id_here

# ScrapingBee API Key (optional, for difficult URLs)
SCRAPINGBEE_API_KEY=your_scrapingbee_api_key_here

# Anthropic API Key (optional, for Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here